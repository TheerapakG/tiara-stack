{
  "name": "typhoon-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "./dist/**"
  ],
  "sideEffects": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "module": "./dist/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier -c .",
    "format:apply": "prettier -w .",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "tsdown -c tsdown.config.ts"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@standard-schema/spec": "^1.0.0"
  },
  "peerDependencies": {
    "effect": "^3.18.4"
  },
  "devDependencies": {
    "@typescript/native-preview": "latest",
    "glob": "^11.0.3",
    "oxlint": "^1.18.0",
    "pathe": "^2.0.3",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  }
}
