{
  "name": "sheet-db-server",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*/index.d.mts",
      "default": "./dist/*/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "tsdown -c tsdown.config.ts && tar -acf dist.tar.zst ./dist"
  },
  "dependencies": {
    "@effect/opentelemetry": "^0.58.0",
    "@effect/platform": "^0.92.1",
    "@effect/platform-node": "^0.98.4",
    "@effect/rpc": "^0.71.1",
    "@opentelemetry/exporter-prometheus": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@rocicorp/zero": "^0.25.9",
    "effect": "^3.18.4",
    "sheet-db-schema": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@typescript/native-preview": "latest",
    "dotenv": "^17.2.3",
    "glob": "^11.0.1",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3"
  }
}
