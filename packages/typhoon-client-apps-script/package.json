{
  "name": "typhoon-client-apps-script",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "module": "./dist/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier -c .",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "rolldown -c rolldown.config.ts"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "peerDependencies": {
    "effect": "^3.17.13",
    "typhoon-core": "workspace:*"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.99",
    "@typescript/native-preview": "latest",
    "glob": "^11.0.1",
    "oxlint": "^1.16.0",
    "pathe": "^2.0.3",
    "prettier": "^3.5.3",
    "rolldown": "1.0.0-beta.36",
    "rolldown-plugin-dts": "^0.16.8",
    "vite": "npm:rolldown-vite@latest"
  }
}
