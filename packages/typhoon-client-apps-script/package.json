{
  "name": "typhoon-client-apps-script",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*/index.d.mts",
      "default": "./dist/*/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "tsdown -c tsdown.config.ts"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.99",
    "@typescript/native-preview": "latest",
    "glob": "^11.0.1",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "effect": "^3.18.4",
    "typhoon-core": "workspace:*",
    "typhoon-server": "workspace:*"
  }
}
