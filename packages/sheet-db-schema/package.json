{
  "name": "sheet-db-schema",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "module": "./dist/*/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier -c .",
    "format:apply": "prettier -w .",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "drizzle-zero generate --output ./src/zero/schema.ts --skip-types --skip-builder && tsdown -c tsdown.config.ts"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@rocicorp/zero": "^0.24.3000000000",
    "drizzle-orm": "^0.44.0",
    "drizzle-zero": "^0.16.1",
    "effect": "^3.18.4",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@typescript/native-preview": "latest",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "oxlint": "^1.16.0",
    "pathe": "^2.0.3",
    "prettier": "^3.5.3",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  }
}
